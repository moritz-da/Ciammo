<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="601.0" styleClass="primary-color" xmlns="http://javafx.com/javafx/16" fx:controller="de.hdm_stuttgart.mi.GuiController.UserOverviewController">
   <children>
      <AnchorPane prefHeight="81.0" prefWidth="601.0" styleClass="secondary-color">
         <children>
            <Label fx:id="welcomeLabel" alignment="CENTER" layoutX="98.0" layoutY="30.0" prefHeight="37.0" prefWidth="405.0" styleClass="text-color" text="                   " textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button layoutX="25.0" layoutY="346.0" mnemonicParsing="false" onAction="#openSettings" styleClass="green-buttons" text="Einstellungen" textFill="WHITE" />
      <Button layoutX="508.0" layoutY="26.0" mnemonicParsing="false" onAction="#logoutUser" styleClass="black-buttons" text="Logout" textFill="#7bcca7" />
      <Button fx:id="joinWgButton" layoutX="238.0" layoutY="255.0" mnemonicParsing="false" onAction="#joinWg" prefHeight="50.0" prefWidth="140.0" style="-fx-border-color: #7bcca7#7bcca7; -fx-border-width: 2px 2px 2px 2px;" styleClass="rectangle-fill" text="WG beitreten">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="showWgButton" layoutX="238.0" layoutY="118.0" mnemonicParsing="false" onAction="#showWg" prefHeight="50.0" prefWidth="140.0" style="-fx-border-color: #7bcca7#7bcca7; -fx-border-width: 2px 2px 2px 2px;" styleClass="rectangle-fill" text="WG anzeigen">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="createWgButton" layoutX="238.0" layoutY="185.0" mnemonicParsing="false" onAction="#createWg" prefHeight="50.0" prefWidth="140.0" style="-fx-border-color: #7bcca7#7bcca7; -fx-border-width: 2px 2px 2px 2px;" styleClass="rectangle-fill" text="WG erstellen">
         <font>
            <Font size="18.0" />
         </font></Button>
   </children>
</AnchorPane>
